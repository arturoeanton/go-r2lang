// Funci√≥n b√°sica
func sumar(a, b) {
    return a + b;
}

// Funci√≥n con l√≥gica compleja
func evaluarNumero(num) {
    if (num % 2 == 0) {
        return "par";
    } else {
        return "impar";
    }
}

// Funci√≥n que usa mapas multilinea
func crearPerfil(nombre, edad) {
    return {
        usuario: {
            nombre: nombre,
            edad: edad,
            activo: true
        },
        configuracion: {
            tema: "claro",
            idioma: "es",
            notificaciones: true
        }
    };
}

// Funci√≥n recursiva
func factorial(n) {
    if (n <= 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

