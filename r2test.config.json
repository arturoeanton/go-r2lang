{
  "testDirs": ["./examples/testing", "./tests", "."],
  "patterns": ["*_test.r2", "*Test.r2", "test_*.r2"],
  "ignore": ["node_modules", "vendor", "*.tmp.r2", ".*"],
  "timeout": "30s",
  "parallel": false,
  "maxWorkers": 4,
  "bail": false,
  "grep": "",
  "tags": [],
  "skip": [],
  "only": [],
  "coverage": {
    "enabled": false,
    "threshold": 80,
    "output": "./coverage",
    "formats": ["console", "html"]
  },
  "reporters": ["console"],
  "verbose": false,
  "retries": 0,
  "watchMode": false,
  "snapshot": {
    "updateSnapshots": false,
    "snapshotDir": "./tests/__snapshots__"
  }
}